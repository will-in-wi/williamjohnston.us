<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <meta http-equiv="X-UA-Compatible" content="ie=edge" />
  <link type="text/plain" rel="author" href="https://www.williamjohnston.us/humans.txt" />
  <title>{% block page_title %}William Johnston for State Representative in SD53A{% endblock %}</title>
  <meta name="description" content="William Johnston for State Representative in SD53A." />
  <link rel="stylesheet" href="/assets/style.min.css" />
  {# <link rel="icon" type="image/png" sizes="64x64" href="/assets/icons/icon64.png">
  <link rel="apple-touch-icon" sizes="180x180" href="/assets/icons/icon180.png"> #}

  <!-- Matomo -->
  <script type="text/javascript">
    var _paq = window._paq || [];
    /* tracker methods like "setCustomDimension" should be called before "trackPageView" */
    _paq.push(['trackPageView']);
    _paq.push(['enableLinkTracking']);
    (function() {
      var u="//data.williamjohnston.us/";
      _paq.push(['setTrackerUrl', u+'matomo.php']);
      _paq.push(['setSiteId', '1']);
      var d=document, g=d.createElement('script'), s=d.getElementsByTagName('script')[0];
      g.type='text/javascript'; g.async=true; g.defer=true; g.src=u+'matomo.js'; s.parentNode.insertBefore(g,s);
    })();
  </script>
<!-- End Matomo Code -->
</head>
<body>
  <div class="page-wrapper">
    <header class="site-header">
      <a href="/">
        {# <img src="/assets/logo.svg" /> #}
        <h1>
          William Johnston<br />
          <small>Candidate for State Representative in HD53A</small>
        </h1>
      </a>
    </header>
    <nav class="site-navigation">
      {# <ul>
        <li class="{% block menu_home_active %}{% endblock %}">
          <a href="/">Home</a>
        </li>
        <li class="{% block menu_donate_active %}{% endblock %}">
          <a href="/donate/">Donate</a>
        </li>
        <li class="{% block menu_positions_active %}{% endblock %}">
          <a href="/positions/">Positions</a>
        </li>
      </ul> #}
    </nav>

    {% block content %}{% endblock %}

    <footer class="site-footer">
      <a class="site-footer_img site-footer_logo-handle" href="https://parler.com/profile/williamformn/">
        <img src="/assets/footer/parler_logo.png" alt="WilliamForMN on Parler" /> @williamformn
      </a>

      <a class="site-footer_img site-footer_logo-handle" href="https://www.facebook.com/williamformn/">
        <img src="/assets/footer/facebook.svg" alt="WilliamForMN on Facebook" /> @williamformn
      </a>

      <a class="site-footer_img site-footer_logo-handle" href="https://twitter.com/williamformn">
        <img src="/assets/footer/twitter.svg" alt="Find me on Twitter" /> @williamformn
      </a>

      {# <a class="site-footer_img" href="https://www.youtube.com/channel/UCKkQALsmjYPGaoC9_JczJUw">
        <img src="/assets/footer/youtube.svg" alt="Watch me on YouTube" />
      </a> #}

      <p class="disclaimer">
        Prepared and paid for by the Campaign of William Johnston committee, 107 Gresham Ave N, Oakdale, MN 55128.
      </p>
    </footer>
  </div>
</body>
</html>
